    {
      "name": "Login",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Accept",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "http://localhost:8000/api/login",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "path": [
            "api",
            "login"
          ],
          "port": "8000"
        },
        "body": {
          "mode": "raw",
          "raw": "{\n    \"email\": \"info@ticarte.com\",\n    \"password\": \"password\",\n    \"device\": \"dev_client\"\n}",
          "options": {
            "raw": {
              "language": "json"
            }
          }
        }
      },
      "event": [
        {
          "listen": "test",
          "script": {
            "type": "text/javascript",
            "exec": [
              "let jsonData = {};",
              "try { jsonData = pm.response.json(); } catch (e) { jsonData = {}; }",
              "",
              "pm.environment.set(\"books.token\", _.get(jsonData, \"token\"));",
              "pm.test(\"Status code is 200\", function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "pm.test(\"Field status equals 1\", function () {",
              "    pm.expect(_.get(jsonData, \"status\")).to.eql(1);",
              "});"
            ]
          }
        }
      ]
